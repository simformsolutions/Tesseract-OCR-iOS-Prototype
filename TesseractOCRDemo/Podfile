# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'TesseractOCRDemo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TesseractOCRDemo

  pod 'TesseractOCRiOS','5.0.1' # for image to text recognition
  pod 'GPUImage','0.1.7' # for image processing
  pod 'IQKeyboardManagerSwift','6.0.4' # to manage keyboard behaviour in textfield and textview
  
end
